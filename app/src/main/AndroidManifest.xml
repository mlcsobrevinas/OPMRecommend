        </activity>
        <activity android:name="com.uplb.mlcsobrevinas.OPMrecommender.SignIn" />
        <activity android:name="com.uplb.mlcsobrevinas.OPMrecommender.SignUp" />
        <activity android:name="com.uplb.mlcsobrevinas.OPMrecommender.personalityTest" />
        <activity android:name=".home" />
        <activity android:name="com.uplb.mlcsobrevinas.OPMrecommender.Start" />
        <activity
            android:name=".home2"
            android:label="@string/title_activity_home2"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.uplb.mlcsobrevinas.OPMrecommender.Home"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme" />
        <activity android:name="com.uplb.mlcsobrevinas.OPMrecommender.SongList" />
